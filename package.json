{
  "name": "ale-viaje-interior",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:gh": "ng build --configuration production --base-href=/",
    "postbuild:gh": "copy dist\\ale-viaje-interior\\browser\\index.html dist\\ale-viaje-interior\\browser\\404.html",
